{
  "name": "openepi-client",
  "version": "1.3.0",
  "description": "A JavaScript client for the openepi data",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ts": "tsc --noEmit",
    "build": "tsup",
    "generate-types": "./generate-types.sh",
    "format": "prettier --write --ignore-path .gitignore .",
    "format:check": "prettier --check --ignore-path .gitignore .",
    "type:check": "tsc --noEmit --pretty",
    "docs": "typedoc ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openearthplatforminitiative/openepi-client-js.git"
  },
  "author": "Erik Dymbe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openearthplatforminitiative/openepi-client-js/issues"
  },
  "homepage": "https://github.com/openearthplatforminitiative/openepi-client-js#readme",
  "devDependencies": {
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.9",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.18.0"
  }
}
